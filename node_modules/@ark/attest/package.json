    "@ark/util": "0.46.0",
    "@ark/fs": "0.46.0"
  },
  "devDependencies": {
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "ts ../repo/build.ts",
    "test": "ts ../repo/testPackage.ts"
  }
}